<% @places.each do |place| %>
  <div class="booyah-box col-xs-10 col-xs-offset-1">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
  </div>
<% end %>

	<div class="col-xs-4">
		<h2><%= paginate @places %></h2>
		<span style="position: absolute; bottom: 0pt; right: 0pt;>"</span>
